datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}

model Products {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  price Float
}

model Users {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  name String
  username String
  password String
  email String
}